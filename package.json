{
  "name": "trakbon-docs",
  "version": "1.0.0",
  "description": "Full stack document management system, complete with roles and privileges.",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --exec babel-node -- ./bin/www.js",
    "mocha": "mocha",
    "start": "npm run start:dev",
    "babel": "babel",
    "webpack": "webpack",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:server": "babel server -d dist/server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js --progress --profile --colors",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:integration": "npm run db:migrate:test && npm run db:seed:test && npm run test-backend",
    "test-backend": "NODE_ENV=test nyc --reporter=html mocha \"./**/server/tests/**/*.spec.js\" --compilers js:babel-register --timeout 60000",
    "test": "NODE_ENV=test npm run test:integration && npm run coverage",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "db:seed:test": "NODE_ENV=test sequelize db:seed:all",
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "sequelize": "sequelize"
  },
  "keywords": [
    "Document",
    "management",
    "system"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andela-tezebuike/Trak-Bon-Docs.git"
  },
  "author": "Tracy ezebuike",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "faker": "^4.1.0",
