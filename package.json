    "material-design": "0.0.0",
    "morgan": "^1.8.1",
    "open": "0.0.5",
    "path": "^0.12.7",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-mdl": "^1.10.2",
    "react-quill": "^1.0.0-rc.2",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.1.1",
